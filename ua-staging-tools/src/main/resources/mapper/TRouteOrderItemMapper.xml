<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.person.ua.dao.mapper.TRouteOrderItemMapper">


    <delete id="deleteByIds">
        delete from t_route_order_item where id in
        <foreach collection="ids" open="(" close=")" index="idx" item="id" separator=",">
            #{id}
        </foreach>
    </delete>

    <select id="queryIdList" resultType="java.lang.Long">
        select id from t_route_order_item limit #{size}
    </select>

</mapper>
